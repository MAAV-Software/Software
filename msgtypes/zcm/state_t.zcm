/**
 * ZCM type for GNC state
 */
struct state_t
{
	int64_t utime;

	double covariance[15][15]

	// State variables
	double attitude[4];
	double position[3];
	double velocity[3];

	// Control inputs
	double angular_velocity[3];
	double acceleration[3];

	// Biases
	double gyro_biases[3];
	double accel_biases[3];
}
