struct roomba_states_t
{
	int64_t utime;
	roomba_state_t current_pos_estimate[10];
	int8_t is_kcf[10]; // true if current pos is from kcf, false if from pf
	int8_t num_grids;
	int64_t future_utime;
	roomba_state_t future_pos_estimate[10];
	float current_state_distribution[num_grids][576];
	float current_heading_distribution[num_grids][576];
	float future_state_distribution[num_grids][576];
	float future_heading_distribution[num_grids][576];
}
